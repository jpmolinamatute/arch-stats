    "debug.javascript.terminalOptions": {
        "resolveSourceMapLocations": [
            "${workspaceFolder}/frontend/**",
            "!${workspaceFolder}/frontend/node_modules/**",
            "${workspaceFolder}/frontend/node_modules/.vite-temp/**"
        ]
    },
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "eslint.enable": true,
    "eslint.validate": [
        "javascript",
        "typescript",
        "vue"
    ],
    "eslint.workingDirectories": [
        "./frontend"
    ],
    "eslint.format.enable": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.fixAll.eslint": "explicit",
        "source.fixAll.markdownlint": "explicit"
    },
    "markdownlint.config": {
        "MD003": {
            "style": "atx"
        },
        "MD030": {
            "ul_single": 1,
            "ol_single": 1,
            "ul_multi": 1,
            "ol_multi": 1
        },
        "MD007": {
            "indent": 4
        },
        "MD013": {
            "line_length": 100
        },
        "no-hard-tabs": true
    },
    "markdown.preview.scrollEditorWithPreview": false,
    "markdown.preview.scrollPreviewWithEditor": false,
    "mypy-type-checker.args": [
        "--config-file",
        "${workspaceFolder}/backend/pyproject.toml"
    ],
    "mypy-type-checker.interpreter": [
        "${workspaceFolder}/backend/.venv/bin/python"
    ],
    "mypy-type-checker.cwd": "${workspaceFolder}/backend",
    "mypy-type-checker.path": [
        "${workspaceFolder}/backend/.venv/bin/mypy"
    ],
    "mypy-type-checker.ignorePatterns": [
        "**/ignore/",
        "**/.venv/",
        "**/__pycache__/",
        "**/.mypy_cache/",
        "**/.pytest_cache/"
    ],
    "python.analysis.exclude": [
        "**/ignore/",
        "**/.git/",
        "**/.venv/",
        "**/__pycache__/",
        "**/.mypy_cache/",
        "**/.pytest_cache/"
    ],
    "python.analysis.extraPaths": [
        "${workspaceFolder}/backend/src"
    ],
    "python.analysis.diagnosticSeverityOverrides": {
        "reportCallIssue": "none"
    },
    "python.analysis.typeCheckingMode": "standard",
    "python.testing.pytestEnabled": true,
    "python.testing.pytestArgs": [
        "--config-file=${workspaceFolder}/backend/pyproject.toml"
    ],
    "python.testing.cwd": "${workspaceFolder}/backend",
    "python.testing.pytestPath": "${workspaceFolder}/backend/.venv/bin/pytest",
    "python.testing.unittestEnabled": false,
    "python.analysis.typeEvaluation.deprecateTypingAliases": true,
    "shellcheck.customArgs": [
        "--shell=bash",
        "--color=never",
        "--external-sources"
    ],
    "shellformat.path": "/usr/bin/shfmt",
    "shellformat.flag": "-i 4 -w",
    "sqlfluff.dialect": "postgres",
    "terminal.integrated.env.linux": {
        "PYTHONPATH": "${workspaceFolder}/backend/src"
    },
    "typescript.tsdk": "./frontend/node_modules/typescript/lib",
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "typescript.format.placeOpenBraceOnNewLineForFunctions": true,
    "vitest.rootConfig": "./frontend/vitest.config.ts",
    "ruff.configuration": "${workspaceFolder}/backend/pyproject.toml",
    "ruff.importStrategy": "fromEnvironment",
    "ruff.interpreter": [
        "${workspaceFolder}/backend/.venv/bin/python"
    ],
    "ruff.path": [
        "${workspaceFolder}/backend/.venv/bin/ruff"
    ],
    "[markdown]": {
        "editor.unicodeHighlight.nonBasicASCII": true,
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "[javascript]": {
        "editor.insertSpaces": true,
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[vue]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[html]": {
        "editor.suggest.insertMode": "replace",
        "editor.insertSpaces": true,
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[python]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "workbench.externalBrowser": "/opt/google/chrome/chrome",
    "workbench.editor.restoreViewState": false,
    "terminal.integrated.tabs.title": "${process}:${task}",
    "eslint.useESLintClass": true,
    "python.terminal.activateEnvInCurrentTerminal": false,
    "python-envs.defaultPackageManager": "",
    "python.terminal.activateEnvironment": false,
    "python-envs.defaultEnvManager": ""
}
