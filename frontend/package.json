{
  "name": "arch-stats_frontend",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint --fix .",
    "clear:vite_cache": "rm -rf ./node_modules/.vite",
    "generate:types": "openapi-typescript http://localhost:8000/api/openapi.json --export-type --immutable --output src/types/types.generated.ts",
    "test": "NODE_OPTIONS='--localstorage-file=./.vitest-localstorage' vitest run",
    "test:watch": "NODE_OPTIONS='--localstorage-file=./.vitest-localstorage' vitest",
    "test:coverage": "NODE_OPTIONS='--localstorage-file=./.vitest-localstorage' vitest run --coverage"
  },
  "dependencies": {
    "vue": "^3.5.24",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.4.1",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.14",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "jsdom": "^27.2.0",
    "openapi-typescript": "^7.10.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.14",
    "vue-tsc": "^3.1.3"
  }
}
